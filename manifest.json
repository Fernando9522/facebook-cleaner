{
  "name": "Facebook Privacy Cleaner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Cleans out your Facebook data after each session.",
  "permissions": [
    "https://www.facebook.com/",
    "http://www.facebook.com/",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "media/16.png",
      "38": "media/48.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "media/128.png",
    "16": "media/16.png",
    "48": "media/48.png"
  },
  "content_scripts": [{
      "matches": ["https://*.facebook.com/ads/preferences/*", "http://*.facebook.com/ads/preferences/*"],
      "js": ["libs/underscore.min.js", "libs/jquery.js", "clean-facebook.js"]
    }
  ]
}
